@model IEnumerable<Green.Models.Rooms>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DoctorModel.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MyRooms.Category)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RoomName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DoctorModel.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MyRooms.Category)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RoomName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ids }) |
            @Html.ActionLink("Details", "Details", new { id=item.ids }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ids })
        </td>
    </tr>
}

</table>
